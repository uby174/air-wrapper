{
  "name": "ai-wrapper",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "dev": "turbo run dev --parallel --filter=@ai-wrapper/web --filter=@ai-wrapper/api",
    "build": "turbo run build",
