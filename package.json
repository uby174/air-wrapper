  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "pnpm": "10.4.1",
    "prettier": "^3.5.1",
    "turbo": "^2.4.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.56.1"
  }
}
